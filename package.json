{
  "name": "hearit-desktop",
  "productName": "Hearit Player",
  "version": "0.0.1",
  "description": "Communist music player",
  "license": "MIT",
  "repository": "sebbert/hearit-desktop",
  "main": "index.js",
  "author": {
    "name": "Sebastian Reinhard",
    "email": "sebastian@sebbert.me",
    "url": "hearit.in"
  },
  "scripts": {
    "test": "xo",
    "start": "electron build",
    "clean": "npm run clean:build && npm run clean:dist",
    "clean:build": "rimraf build",
    "clean:dist": "rimraf dist",
    "dist:webpack": "env NODE_ENV=production webpack",
    "dist:electron": "electron-packager ./build --platform=all --arch=x64 --prune --overwrite --out=dist",
    "copy-dependencies": "cp -R node_modules build/node_modules",
    "dist": "npm run dist:webpack && npm run dist:electron"
  },
  "keywords": [
    "hearit",
    "party",
    "music"
  ],
  "dependencies": {
    "electron-debug": "^1.0.0",
    "electron-json-storage": "^2.0.0",
    "express": "^4.13.4",
    "firebase": "^3.1.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.12.0",
    "material-ui": "^0.15.1",
    "node-spotify-webhelper": "0.0.4",
    "normalize.css": "^4.1.1",
    "react": "^15.2.1",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "rx": "^4.1.0",
    "velocity-react": "^1.1.5"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.1",
    "devtron": "^1.1.0",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^1.0.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "xo": "^0.15.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
